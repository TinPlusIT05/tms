<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<!-- List view -->
		<record id="view_import_attendance_error_tree" model="ir.ui.view">
			<field name="name">view.import.attendance.error.tree</field>
			<field name="model">import.attendance.error</field>
			<field name="arch" type="xml">
				<tree string="Imported Attendance Error" >
					<field name="employee_id"/>
					<field name="date"/>
					<field name="action"/>
					<field name="action_reason_id"/>
					<field name="error"/>
				</tree>
			</field>
		</record>
		
		<record id="action_import_attendance_error" model="ir.actions.act_window">
			<field name="name">Imported Attendance Error</field>
			<field name="res_model">import.attendance.error</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree</field>
		</record>
	</data>
</openerp>