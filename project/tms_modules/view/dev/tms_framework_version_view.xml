<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record id="view_tms_framework_version_tree" model="ir.ui.view">
			<field name="name">tms.framework.version.tree</field>
			<field name="model">tms.framework.version</field>
			<field name="type">tree</field>
			<field name="arch" type="xml">
				<tree string="Framework Version">
					<field name="name" />
					<field name="project_type_id" />
				</tree>
			</field>
		</record>

        <record id="view_tms_framework_version_filter" model="ir.ui.view">
            <field name="name">tms.framework.version.filter</field>
            <field name="model">tms.framework.version</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Search Framework Version">
					<field name="name" string="Name" />
					<field name="project_type_id" string="Project Type" />
                </search>
            </field>
        </record>

		<record id="view_tms_framework_version_form" model="ir.ui.view">
			<field name="name">tms.framework.version.form</field>
			<field name="model">tms.framework.version</field>
			<field name="type">form</field>
			<field name="arch" type="xml">
				<form string="Framework Version">
					<field name="name" />
					<field name="project_type_id" />
				</form>
			</field>
		</record>

		<record id="action_view_tms_framework_version_tree" model="ir.actions.act_window">
			<field name="name">Framework Version</field>
			<field name="res_model">tms.framework.version</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
		</record>
	</data>
</openerp>

